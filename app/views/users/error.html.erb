                                        <% @g.each do |h| %> 
                                        
        
                     
                       
                                        <tr>
                          
                                  <td><%= h[0] %></td>
                                  <td><%= h[1] %></td>
                                  <td><%= h[2] %></td>
                                  <td><%= h[3] %></td>
                                  <td><%= h[4] %></td>
                                  
                                  <th> <%= link_to "click", users_admin_path(h[0])%>



                                    <button type="button" class="btn btn-info" data-toggle="modal" data-target="#myModal">View Details</button></th>
                                      </tr>
 <% end %>
                                    </tbody>
